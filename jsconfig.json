{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": " ./",
        "paths": {
            "@components/": [
                "src/components/"
            ],
            "@containers/": [
                "src/containers/"
            ],
            "@context/": [
                "src/context/"
            ],
            "@hooks/": [
                "src/hooks/"
            ],
            "@routes/": [
                "src/routes/"
            ],
            "@styles/": [
                "src/styles/"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}